{
  "name": "nest-geoip2",
  "version": "0.0.0-development",
  "description": "MaxMind GeoIP2 module for Nest framework.",
  "keywords": [
    "nest",
    "nestjs",
    "geoip2",
    "maxmind"
  ],
  "author": {
    "name": "Xudong Huang",
    "email": "me@xudong.dev",
    "url": "https://www.xudong.dev/"
  },
  "bugs": "https://github.com/xudongdev/nest-geoip2/issues",
  "homepage": "https://github.com/xudongdev/nest-geoip2#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xudongdev/nest-geoip2.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "nest build",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@maxmind/geoip2-node": "^3.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "reflect-metadata": "^0.1.0"
  },
  "devDependencies": {
    "@maxmind/geoip2-node": "^3.2.0",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{json,js,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "airbnb-base",
      "plugin:import/typescript",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "jest",
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/sort": "error",
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": "off",
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "ts": "never"
        }
      ]
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
